package com.it.bean;

public class Project_task {
    private int id;
    private int project_id;
    private int project_work_id;
    private String user_ids;
    private int manager_id;
    private String name;
    private int plan_time;
    private int actul_time;
    private int status;
    private String descrption;

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public int getProject_id() {
        return project_id;
    }

    public void setProject_id(int project_id) {
        this.project_id = project_id;
    }

    public int getProject_work_id() {
        return project_work_id;
    }

    public void setProject_work_id(int project_work_id) {
        this.project_work_id = project_work_id;
    }

    public String getUser_ids() {
        return user_ids;
    }

    public void setUser_ids(String user_ids) {
        this.user_ids = user_ids;
    }

    public int getManager_id() {
        return manager_id;
    }

    public void setManager_id(int manager_id) {
        this.manager_id = manager_id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getPlan_time() {
        return plan_time;
    }

    public void setPlan_time(int plan_time) {
        this.plan_time = plan_time;
    }

    public int getActul_time() {
        return actul_time;
    }

    public void setActul_time(int actul_time) {
        this.actul_time = actul_time;
    }

    public int getStatus() {
        return status;
    }

    public void setStatus(int status) {
        this.status = status;
    }

    public String getDescrption() {
        return descrption;
    }

    public void setDescrption(String descrption) {
        this.descrption = descrption;
    }

    @Override
    public String toString() {
        return "Project_task{" +
                "id=" + id +
                ", project_id=" + project_id +
                ", project_work_id=" + project_work_id +
                ", user_ids='" + user_ids + '\'' +
                ", manager_id=" + manager_id +
                ", name='" + name + '\'' +
                ", plan_time=" + plan_time +
                ", actul_time=" + actul_time +
                ", status=" + status +
                ", descrption='" + descrption + '\'' +
                '}';
    }

    public Project_task(int id, int project_id, int project_work_id, String user_ids, int manager_id, String name, int plan_time, int actul_time, int status, String descrption) {
        this.id = id;
        this.project_id = project_id;
        this.project_work_id = project_work_id;
        this.user_ids = user_ids;
        this.manager_id = manager_id;
        this.name = name;
        this.plan_time = plan_time;
        this.actul_time = actul_time;
        this.status = status;
        this.descrption = descrption;
    }

    public Project_task() {
    }
}
