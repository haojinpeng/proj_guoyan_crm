package com.it.bean;

public class Approval_Process {
    private Integer id;
    private Integer parent_id;
    private String process_name;
    private Integer approval_role;
    private Integer approval_user;
    private Integer approval_department ;
    private Integer type;
    private String remarks;

    public Approval_Process() {
    }

    public Approval_Process(Integer id, Integer parent_id, String process_name, Integer approval_role, Integer approval_user, Integer approval_department, Integer type, String remarks) {
        this.id = id;
        this.parent_id = parent_id;
        this.process_name = process_name;
        this.approval_role = approval_role;
        this.approval_user = approval_user;
        this.approval_department = approval_department;
        this.type = type;
        this.remarks = remarks;
    }

    @Override
    public String
    toString() {
        return "Approval_Process{" +
                "id=" + id +
                ", parent_id=" + parent_id +
                ", process_name='" + process_name + '\'' +
                ", approval_role=" + approval_role +
                ", approval_user=" + approval_user +
                ", approval_department=" + approval_department +
                ", type=" + type +
                ", remarks='" + remarks + '\'' +
                '}';
    }

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getParent_id() {
        return parent_id;
    }

    public void setParent_id(Integer parent_id) {
        this.parent_id = parent_id;
    }

    public String getProcess_name() {
        return process_name;
    }

    public void setProcess_name(String process_name) {
        this.process_name = process_name;
    }

    public Integer getApproval_role() {
        return approval_role;
    }

    public void setApproval_role(Integer approval_role) {
        this.approval_role = approval_role;
    }

    public Integer getApproval_user() {
        return approval_user;
    }

    public void setApproval_user(Integer approval_user) {
        this.approval_user = approval_user;
    }

    public Integer getApproval_department() {
        return approval_department;
    }

    public void setApproval_department(Integer approval_department) {
        this.approval_department = approval_department;
    }

    public Integer getType() {
        return type;
    }

    public void setType(Integer type) {
        this.type = type;
    }

    public String getRemarks() {
        return remarks;
    }

    public void setRemarks(String remarks) {
        this.remarks = remarks;
    }
}
